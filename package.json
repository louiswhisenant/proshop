{
  "name": "proshop-v2",
  "version": "2.0.0",
  "description": "eCommerce application built with the MERN stack.",
  "type": "module",
  "main": "server.js",
  "scripts": {
    "start": "node backend/server.js",
    "server": "nodemon backend/server.js",
    "client": "npm start --prefix frontend",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "data:import": "node backend/seeder.js -import",
    "data:destroy": "node backend/seeder.js -destroy"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/louiswhisenant/proshop.git"
  },
  "author": "Louis Whisenant",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/louiswhisenant/proshop/issues"
  },
  "homepage": "https://github.com/louiswhisenant/proshop#readme",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "colors": "^1.4.0",
    "express": "^4.18.2",
    "mongoose": "^8.0.0"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "dotenv": "^16.3.1",
    "nodemon": "^3.0.1"
  }
}